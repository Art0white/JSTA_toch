<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "mybatis-3-mapper.dtd">
<mapper namespace="com.xq.tmall.dao.AfterSaleMapper">

    <insert id="insertAfterSale" parameterType="com.xq.tmall.entity.AfterSale">
        insert into after_sale(after_sale_number,after_sale_user_id,after_user_state,after_sale_picture)
        values(#{afterSale.afterSaleNumber},#{afterSaleUserId},#{afterSaleState},#{afterSalePicture});
    </insert>
</mapper>
